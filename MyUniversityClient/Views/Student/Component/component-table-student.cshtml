<script type="text/template" id="component-table-student">
    <table class="table table-bordered text-center mt-3">
        <%console.log(data)%>
        <thead class "table-secondary">
            <tr>
                <th>Student ID</th>
                <th>Name</th>
                <th>Age</th>
                <th>Major Name</th>
            </tr>
        </thead>
        <tbody>
            <% if (data.length > 0) { %>
                <% _.each(data, function(item, index) { %>
                    <tr>
                        <td><%= item.studentId %></td>
                        <td><%= item.name %></td>
                        <td><%= item.age %></td>
                        <td><%= item.majorName %></td>
                    </tr>
                <% }); %>
            <% } else { %>
                <tr>
                    <td colspan="4" class="text-center">No data found</td>
                </tr>
            <% } %>
        </tbody>
    </table>
</script>